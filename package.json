{
  "name": "Alconomy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./web/bin/www",
    "start-debug": "DEBUG=Alconomy:* npm start",
    "start-native": "react-native start",
    "ios-bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "android-bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "web-bundle": "cross-env NODE_ENV=production webpack --config web/webpack/web.prod.config.js --progress --colors",
    "web-dev": "webpack-dev-server --content-base web/public/ --config web/webpack/web.dev.config.js --port 3001 --inline --hot --colors",
    "web-dev-express": "cross-env NODE_ENV=development webpack --watch --config web/webpack/web.dev.config.js --hot --colors",
    "clear-cache": "gulp clear-cache"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "body-parser": "~1.18.2",
    "classnames": "^2.2.5",
    "clone": "^2.1.1",
    "color": "^2.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "express": "~4.16.2",
    "firebase": "^4.6.0",
    "hjs": "~0.0.6",
    "jquery": "^3.2.1",
    "keycode": "^2.1.9",
    "material-ui": "^1.0.0-beta.17",
    "material-ui-icons": "^1.0.0-beta.17",
    "morgan": "~1.9.0",
    "prop-types": "^15.6.0",
    "qrcode.react": "^0.7.2",
    "react": "^16.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-native": "^0.49.3",
    "react-native-storage": "^0.2.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-swipeable-views": "^0.12.8",
    "react-touch": "^0.4.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "~2.4.5",
    "superagent": "^3.6.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint-config-rallycoding": "^3.2.0",
    "gulp": "^3.9.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.4",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "react-native-cli": "^2.0.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.3.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
    "webpack": "^3.7.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-hot-middleware": "^2.20.0"
  }
